{
  "name": "yai",
  "displayName": "yai",
  "description": "Yet Another Importer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yai.import",
        "title": "YAI: Import Module"
      },
      {
        "command": "yai.index",
        "title": "YAI: Index Modules"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "yaiDependencies",
          "name": "Yai Dependencies"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "preintgtest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "intgtest": "node ./out/test/runTest.js",
    "test": "npx mocha -r ts-node/register ./src/test/spec/*.test.ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
